<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="NetflixPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <non-jta-data-source>java:/comp/env/jdbc/NetflixDB</non-jta-data-source>
    <class>models.Asset</class>
    <class>models.Title</class>
    <class>models.User</class>
    <class>models.Style</class>
    <class>models.Artwork</class>
    <class>models.RoundArtwork</class>
    <class>models.AssetType</class>
    <class>models.Position</class>
    <class>models.Genre</class>
    <class>models.Backup</class>
    <class>models.Feedback</class>
    <class>models.Language</class>
    <class>models.Report</class>
    <class>models.MessageGroup</class>
    <class>models.Skillset</class>
    <class>models.Auser</class>
    <class>models.Account</class>
    <class>models.UserLanguage</class>
    <class>models.UserSkillset</class>
    <class>models.UserMessageGroup</class>
    <class>models.UserTitle</class>
    <class>models.TitleGenre</class>
    <class>models.RecoveryUser</class>
    <class>models.SharedLink</class>
    <class>models.TitleHasAccount</class>
    <class>models.Status</class>
    <class>models.DropboxToken</class>
    <class>models.Country</class>
    <class>models.Message</class>
    <class>models.AccountHasMessageGroup</class>
    <class>models.Location</class>
    <class>models.Round</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
  </persistence-unit>
</persistence>
